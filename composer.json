{
    "name": "laci/skeletons",
    "description": "An artisan command to make skeletons with content for a Laravel application.",
    "version": "1.0.0",
    "keywords": ["laravel", "package", "skeletons", "generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kovács László",
            "email": "laci007@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Laci\\Skeletons\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "suggest": {
        "illuminate/console": "Required for command-line features",
        "illuminate/support": "Provides helper functions"
    },
    "require-dev": {
        "illuminate/console": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Laci\\Skeletons\\SkeletonsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}

